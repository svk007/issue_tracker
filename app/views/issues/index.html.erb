<table class="table table-hover">
  <tr>
    <th>Ticket Number</th>
    <th>Heading</th>
    <th>Description</th>
    <th>Status</th>
  </tr>
  <% @issues.each do |data| %>
  <td><%= data.ticket_no %></td>
  <td><%= data.heading %></td>
  <td><%= data.description %></td>
  <td><%= data.status %></td>
  <td> <%= link_to 'Assign', action: 'show_devs', id: data.id %></td>
</tr>
<% end %>
</table>